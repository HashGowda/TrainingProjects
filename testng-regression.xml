<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Sauce Las Test Automation Regression Suite"
       thread-count="3" parallel="tests" verbose="3">


    <test name="Sauce Test - Edge">
        <parameter name="browser" value="Edge" />
        <parameter name="version" value="112.0" />
        <parameter name="platform" value="windows 10" />

        <classes>
            <class name="BrowserStack.SauceTest" />
        </classes>

    </test>

    <test name="Sauce Test - Chrome">
        <parameter name="browser" value="Chrome" />
        <parameter name="version" value="112.0" />
        <parameter name="platform" value="windows 10" />

        <classes>
            <class name="BrowserStack.SauceTest" />
        </classes>

    </test>
</suite>